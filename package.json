  "homepage": "https://github.com/Marwan-Alsayadi/Tetris-Game",
  "repository": {
    "type": "git",
    "url": "https://github.com/Marwan-Alsayadi/Tetris-Game.git"
  },
  "bugs": {
    "url": "https://github.com/Marwan-Alsayadi/Tetris-Game/issues"
  }
